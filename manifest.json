{
    "manifest_version": 3,
    "name": "Da holda",
    "description": "Holds da cookie",
    "version": "1.0",
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "permissions": ["scripting", "activeTab", "cookies"],
    "action": {
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "host_permissions": ["http://*/*", "https://*/*"]
}